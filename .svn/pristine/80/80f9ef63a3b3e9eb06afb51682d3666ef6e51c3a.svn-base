.sectionbox .ping_more a { display:block; margin-top:20px; height:31px; line-height:31px; text-align:center; font-weight:bold; background-color:#f4f4f4; border:1px solid #e4e4e4; }
.sectionbox .ping_more a span { color:#666; font-weight:normal; }
.sectionbox .ping_more a:hover { background-color:#eee; text-decoration:none; }

/*商品详情*/
.spxq_main{font-size: 14px; line-height: 24px;}
.spxq_main .nala_say{line-height: 28px; background:url(http://img.nala.com.cn/lizi/b2c/images/nalasay-bg.jpg) 0 0 no-repeat; padding: 0 20px 0 140px; min-height: 110px; +height: 110px;}
.spxq_main img{display: block; margin: 0 auto; max-width: 750px;}
.spxq_main .spxq_top{margin: 25px 0; font-size: 16px; color: #f70; font-weight: bold;}
.spxq_main .spxq_dec div{margin-bottom: 20px;}

.spxq_main table{width: 100%;}
.spxq_main table td{border: 1px solid #e5e5e5; padding:5px 10px; }
.spxq_main table td.th{background-color: #f5f5f5; font-weight: bold; width: 70px; text-align: right;}
.spxq_main table td strong{font-weight: 400;}
.spxq_main table td img{width: 200px; height: 200px;}
.spxq_main table td div,
.spxq_main table td span{display: block; margin-bottom: 4px;}
.spxq_main h3{font-size: 20px; margin: 20px 0 10px; color: #f70; border-bottom: 1px solid #f70; padding-bottom: 5px; }
.spxq_main p{margin-bottom: 10px;}

#ppsqitem div{text-align: center;}
/*详情右侧区域*/
.right_planes {float:right; width:206px; padding-top: 15px; }
.right_planes .plane {border:solid 1px #E6E6E6; margin-top:15px; zoom:1; overflow:hidden; }
.right_planes .header { height:30px; line-height:30px; overflow:hidden; padding:0 10px; border-bottom:solid 1px #E6E6E6; background:#f1f1f1; }
.right_planes .header h5{font-size: 14px;}
.right_planes .product_list { width:172px; margin:0 auto; }
.right_planes .product_list li { position:relative; margin-top: 15px;}
.right_planes .product_list li img{margin: 0 auto;}
.right_planes .plane .plane_con { padding:10px; }

/*右侧热销排行榜*/
.sell_hot li { padding:10px 0 10px 16px; zoom: 1; position:relative; border-bottom:dashed 1px #eee; }
.sell_hot li .product_img { display:block; height:55px; width:55px; border:solid 1px #ECECEC; }
.sell_hot li .product_property { float:right; width:104px; line-height: 14px; }
.sell_hot li .product_property a{display: block; height: 27px; overflow: hidden;}
.sell_hot li .number { position:absolute; left:0px; top:3px; font-size:18px; font-weight:bold; color:#f70; }


/*查看更多商品*/
a.viewbrandproducts { background:#f1f1f1; border: 1px solid #E7E7E7; display:block; padding: 5px 0; margin: 20px auto; width: 100px; text-align: center;}
a.viewbrandproducts:hover { background:#eee ; text-decoration: none; }

/*品牌 品类 功效 切换模块*/
.class_tabs { border:solid 1px #DBDBDB; margin-top: 15px;}
.class_tabs .switchable_nav li { float:left; width:67px; height:28px; line-height:28px; background-color:#F5F5F5; border-bottom:solid 1px #DBDBDB; text-align:center; font-weight:bold; cursor:pointer; }
.class_tabs .switchable_nav .partition { border-left:solid 1px #DBDBDB; border-right:solid 1px #DBDBDB; }
.class_tabs .switchable_nav li.current { width:68px; border-bottom:none; background:#FFF; }
.class_tabs .switchable_content li { padding:6px 0; line-height:22px; font-size: 0; }
.class_tabs .switchable_content li a { display: inline-block; font-size: 12px; margin:0 6px; color:#666; white-space:nowrap; }
.class_tabs .switchable_content li a.root { color:#2E9CDC; }

/*按钮 查看更多*/
a.viewbutton { display: inline-block; padding:5px 12px; border:1px solid #e7e7e7; background-color: #f1f1f1; margin-top: 10px; float: left;}
a.viewbutton:hover { text-decoration:none; }

/*加入购物车成功的弹窗*/
